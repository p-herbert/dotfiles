{
    "ecmaVersion": 6,
    "libs": [
        "browser",
        "ecmascript",
        "react",
        "underscore"
    ],
    "dontLoad": [
        "node_modules/**",
        ".meteor"
    ],
    "loadEagerly": [
        "imports/**/*.js",
        "snippets/**/*.js"
    ],
    "plugins": {
        "node": {},
        "modules": {},
        "es_modules": {},
        "node_resolve": {}
    }
}
